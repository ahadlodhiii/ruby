  <body class="<%= body_class %>" id="<%= @body_id || 'default' %>" data-hook="body">
    <!-- This only removes default margins in our renderer iframe, you may remove -->
    <div class="">
      <%= render partial: 'spree/shared/header' %>
      <div id="overlay" class="overlay hide-on-esc"></div>
      <div data-hook>
        <main id="content" data-hook>
          <%= flash_messages %>
          <%= yield %>
        </main>

        <%= yield :templates %>
      </div>

      <div class="relative flex flex-wrap h-full w-full items-start overflow-clip bg-white  " >
        <div class="flex flex-row w-full  h-28 flex-shrink-0 items-center pl-12  mt-28" >
          <div class="flex flex-col w-96 font-excluded-italic inset-0  items-center self-stretch h-28 text-5xl tracking-[-0.32px] leading-[normal] text-black drop-shadow-lg uppercase" >
            <p><span class="italic">Popular Brands</span></p>
          </div>
        </div>
        <div class="flex flex-row w-full h-[400px] px-2">

          <div class="flex flex-col w-1/3 h-full flex-shrink-0 items-center justify-center" >
            
              <!-- <div class="flex flex-col items-center pr-14"> -->
                <%= image_tag 'homepage/UGG_logo1.svg', class: "h-20 w-60 flex-shrink-0" %>
                
              <!-- </div> -->
          </div>
          <div class="flex flex-col w-1/3  h-full flex-shrink-0 items-start" >
            <%= image_tag 'homepage/bg-untitled-design-21.png', class: "relative h-[260px] w-[459px] flex-shrink-0" %>
                
            
          </div>
          <div class="flex flex-col w-1/3 h-full flex-shrink-0 items-center justify-center" >
            <!-- <div class="flex flex-col items-center pr-14"> -->
              <%= image_tag 'homepage/untitled-design-72.png', class: "relative h-[53px] w-[482px] flex-shrink-0" %>
             
            <!-- </div> -->
          </div>

          
        </div>

        <div class="flex flex-row w-full h-[400px] px-2">
          <div class="flex flex-col w-1/3 items-start  inset-0 flex max-h-full max-w-full bg-no-repeat" >
            <%= image_tag 'homepage/untitled-design-71.png', class: "h-[500px] w-[575px] relative bottom-[6rem]" %>
          </div>
          <div class="flex flex-col w-1/3 h-96 flex-shrink-0 flex-col items-center" >
            <%= image_tag 'homepage/bg-untitled-design-211.png', class: "w-[410px] h-[244px]" %>
          </div>
          <div class="flex flex-col w-1/3  flex-shrink-0 items-start" >

            <%= image_tag 'homepage/sandal.svg', class: "-mb-11 h-[244px] w-[500px] flex-shrink-0" %>
            <!-- <object
              data="<%#= asset_url('/homepage/sandal.svg') %>"
              class="-mb-11 h-[500px] w-[500px] flex-shrink-0"
            ></object> -->
          </div>

          
        </div>
          
          
      </div>

    </div>
  </body>
